[32m+[m[32m                $responseMessage = "You have chosen Waafi services. Please provide the details.";[m
[32m+[m[32m            } elseif ($userMessage === "6") {[m
[32m+[m[32m                // Handle Connect With Agent[m
[32m+[m[32m                $responseMessage = "You have chosen to connect with an agent. Please hold on.";[m
[32m+[m[32m            } elseif ($userMessage === "0") {[m
[32m+[m[32m                // Go back option[m
[32m+[m[32m                $this->sessionData[$from]['menu_state'] = 'main'; // Reset to main menu[m
[32m+[m[32m                $responseMessage = "Going back to the main menu.";[m
[32m+[m[32m            } else {[m
[32m+[m[32m                // Handle invalid option[m
[32m+[m[32m                $responseMessage = "Invalid option. Please select a valid submenu option.";[m
[32m+[m[32m            }[m
[32m+[m[41m                [m
[32m+[m[32m        } elseif ($this->sessionData[$from]['menu_state'] === 'sim_card') {[m
[32m+[m[32m            // Handle Sim-card submenu selections[m
[32m+[m[32m            switch ($userMessage) {[m
[32m+[m[32m                case '1':[m
[32m+[m[32m                    $responseMessage = "Mushaax Information...";[m
[32m+[m[32m                    break;[m
[32m+[m[32m                case '2':[m
[32m+[m[32m                    $responseMessage = "Please enter your phone number for Pin/Puk:";[m
[32m+[m[32m                    $this->sessionData[$from]['menu_state'] = 'pin_puk_number_entry';[m
[32m+[m[32m                    break;[m
[32m+[m[32m                case '0':[m
[32m+[m[32m                    $responseMessage = "Going back to the main menu...\n" . implode("\n", $mainMenu);[m
[32m+[m[32m                    $this->sessionData[$from]['menu_state'] = 'main';[m
[32m+[m[32m                    break;[m
[32m+[m[32m                default:[m
[32m+[m[32m                    $responseMessage = "Please select a valid option.";[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Handle Pin/Puk number entry[m
[32m+[m[32m        if ($this->sessionData[$from]['menu_state'] === 'pin_puk_number_entry') {[m
[32m+[m[32m            if (is_numeric($userMessage) && strlen($userMessage) === 9) {[m
[32m+[m[32m                $this->sessionData[$from]['pin_puk_number'] = $userMessage;[m
[32m+[m[32m                $apiResponse = $this->callPinPukAPI($this->sessionData[$from]['pin_puk_number']);[m
[32m+[m[41m                [m
[32m+[m[32m                if ($apiResponse['status'] === 'success') {[m
