[32m+[m[32m                $responseMessage = "You have chosen Merchant Information. Please specify your merchant details.";[m
[32m+[m[32m            } elseif ($userMessage === "3") {[m
[32m+[m[32m                // Handle Wrong ZAAD Transfer Support[m
[32m+[m[32m                $responseMessage = "You have chosen Wrong ZAAD Transfer Support. Please describe your issue.";[m
[32m+[m[32m            } elseif ($userMessage === "4") {[m
[32m+[m[32m                // Handle Last ZAAD Transactions[m
[32m+[m[32m                $responseMessage = "You have chosen Last ZAAD Transactions. Fetching your last transactions...";[m
[32m+[m[32m            } elseif ($userMessage === "5") {[m
