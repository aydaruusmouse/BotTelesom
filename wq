[32m+[m[32m                } else {[m
[32m+[m[32m                    $responseMessage = "Error: " . $apiResponse['message'];[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                $this->sessionData[$from]['menu_state'] = 'sim_card'; // Return to Sim Card menu[m
[32m+[m[32m                unset($this->sessionData[$from]['pin_puk_number']); // Reset stored number[m
[32m+[m[32m            } else {[m
[32m+[m[32m                $responseMessage = "Please enter a valid 9-digit number:";[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $responseMessage;[m
[32m+[m[32m    }[m
[32m+[m
