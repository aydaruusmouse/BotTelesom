[32m+[m[32m        "wrongnumber" => $wrongNumber,[m
[32m+[m[32m        "currency_code" => $currencyCode[m
[32m+[m[32m    ];[m
[32m+[m[32m        // Convert the request body to JSON format[m
[32m+[m[32m        $jsonRequestBody = json_encode($apiRequestBody);[m
[32m+[m
[32m+[m[32m        // Set up the API request headers[m
[32m+[m[32m        $apiHeaders = [[m
[32m+[m[32m            'apiTokenUser: mob#!Billing!*',[m
[32m+[m[32m            'apiTokenPwd: De6$A7#ES282S@m@l!n.2BIoz',[m
[32m+[m[32m            'Content-Type: application/json',[m
[32m+[m[32m        ];[m
[32m+[m[41m    [m
[32m+[m[32m        // Initialize cURL session for the API request[m
[32m+[m[32m        $ch = curl_init('http://10.10.0.7:8077/api/KaaliyeApi/BlockWrongTransaction');[m
[32m+[m[32m        curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);[m
[32m+[m[32m        curl_setopt($ch, CURLOPT_POST, true);[m
[32m+[m[32m        curl_setopt($ch, CURLOPT_POSTFIELDS, $jsonRequestBody);[m
[32m+[m[32m        curl_setopt($ch, CURLOPT_HTTPHEADER, $apiHeaders);[m
[32m+[m[41m    [m
[32m+[m[32m        // Execute the API request[m
[32m+[m[32m        $apiResponse = curl_exec($ch);[m
[32m+[m[41m    [m
[32m+[m[32m        // Check for cURL errors[m
[32m+[m[32m        if (curl_errno($ch)) {[m
[32m+[m[32m            $responseMessage = "Error connecting to the API: " . curl_error($ch);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            // Parse the API response[m
[32m+[m[32m            $decodedResponse = json_decode($apiResponse, true);[m
[32m+[m[41m    [m
[32m+[m[32m            // Check for a successful response[m
[32m+[m[32m            if ($decodedResponse['status'] == '1') {[m
[32m+[m[32m                $responseMessage = $decodedResponse['Message']; // Display success message from the API[m
[32m+[m[32m            } else {[m
[32m+[m[32m                $responseMessage = "Failed to block the wrong transaction. Please try again.";[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m    [m
[32m+[m[32m        // Close the cURL session[m
[32m+[m[32m        curl_close($ch);[m
[32m+[m[41m    [m
[32m+[m[32m        // Reset the menu state to the main ZAAD menu after completing the API request[m
[32m+[m[32m        $_SESSION['menu_state'] = 'zaad';[m
[32m+[m[41m    [m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32melseif($_SESSION['menu_state'] === 'sim_card') {[m
[32m+[m[32m    if ($userMessage === '1') {[m
[32m+[m[32m        $responseMessage = "mushaax";[m
