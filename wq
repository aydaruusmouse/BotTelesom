[32m+[m[32m        } elseif ($this->sessionData[$from]['menu_state'] === 'zaad') {[m
[32m+[m[32m            // Handle ZAAD submenu selections[m
[32m+[m[32m            if ($userMessage === "1") {[m
[32m+[m[32m                // Handle New ZAAD Account[m
