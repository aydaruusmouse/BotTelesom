[32m+[m[32m    } else {[m
[32m+[m[32m        $responseMessage = "Invalid option. Please select the correct money type.";[m
[32m+[m[32m    }[m
[32m+[m[32m} elseif ($_SESSION['menu_state'] === 'zaad_wrong_transfer_receiver_number') {[m
[32m+[m[32m    $_SESSION['zaad_wrong_transfer_receiver_number'] = $userMessage; // Store receiver's number[m
[32m+[m[32m    $responseMessage = "Please enter the transaction number:";[m
[32m+[m[32m    $_SESSION['menu_state'] = 'zaad_wrong_transfer_transaction_number'; // Set state for transaction number[m
[32m+[m[32m} elseif ($_SESSION['menu_state'] === 'zaad_wrong_transfer_transaction_number') {[m
[32m+[m[32m    // Finalize wrong transfer[m
[32m+[m
[32m+[m[32m    $apiRequestBody = [[m
[32m+[m[32m        "msisdn" => '634097961',[m
