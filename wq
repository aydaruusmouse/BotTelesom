[32m+[m[32m        if ($apiResponse['status'] === 'success') {[m
[32m+[m[32m            $responseMessage = "Pin/Puk details for number: " . $this->sessionData[$from]['pin_puk_number'] . "\nResponse: " . $apiResponse['message'];[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $responseMessage = "Error: " . $apiResponse['message'];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Reset session state to 'sim_card' and clean up stored phone number[m
[32m+[m[32m        $this->sessionData[$from]['menu_state'] = 'sim_card';[m
[32m+[m[32m        unset($this->sessionData[$from]['pin_puk_number']); // Clear the stored number[m
[32m+[m[32m    } else {[m
[32m+[m[32m        // Validation for invalid input[m
[32m+[m[32m        $responseMessage = "Please enter a valid 9-digit number:";[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m        // Output the response message (for testing)[m
[32m+[m[32m        echo $responseMessage;[m
[32m+[m[41m    [m
[32m+[m[32m        // Output response[m
[32m+[m[32m        return back()->with('response', $responseMessage);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m
[32m+[m[32mprivate function callPingBukAPI($phoneNumber)[m
[32m+[m[32m{[m
[32m+[m[32m    // Initialize the cURL session[m
[32m+[m[32m    $curl = curl_init();[m
[32m+[m
[32m+[m[32m    // Prepare the data to be sent via POST[m
[32m+[m[32m    $postData = json_encode([[m
[32m+[m[32m        "Callsub" => $phoneNumber,[m
[32m+[m[32m        "UserId" => "imll",[m
[32m+[m[32m    ]);[m
[32m+[m
[32m+[m[32m    // Set cURL options[m
[32m+[m[32m    curl_setopt_array($curl, [[m
[32m+[m[32m        CURLOPT_URL => "http://10.55.1.143:8983/api/CRMApi/GetSimDetails",[m
[32m+[m[32m        CURLOPT_RETURNTRANSFER => true,[m
[32m+[m[32m        CURLOPT_ENCODING => "",[m
[32m+[m[32m        CURLOPT_MAXREDIRS => 10,[m
[32m+[m[32m        CURLOPT_TIMEOUT => 30,[m
[32m+[m[32m        CURLOPT_HTTP_VERSION => CURL_HTTP_VERSION_1_1,[m
[32m+[m[32m        CURLOPT_CUSTOMREQUEST => "POST",[m
[32m+[m[32m        CURLOPT_POSTFIELDS => $postData,[m
[32m+[m[32m        CURLOPT_HTTPHEADER => [[m
[32m+[m[32m            "apiTokenUser: CRMUser",[m
[32m+[m[32m            "apiTokenPwd: ZEWOALJNADSLLAIE321@!",[m
[32m+[m[32m            "Content-Type: application/json"[m
[32m+[m[32m        ],[m
[32m+[m[32m    ]);[m
[32m+[m
[32m+[m[32m    // Execute the request and get the response[m
[32m+[m[32m    $response = curl_exec($curl);[m
[32m+[m[32m    $err = curl_error($curl);[m
[32m+[m
[32m+[m[32m    // Close the cURL session[m
[32m+[m[32m    curl_close($curl);[m
[32m+[m
[32m+[m[32m    // Log the error and return an error message if there's a cURL error[m
[32m+[m[32m    if ($err) {[m
[32m+[m[32m        \Log::error("cURL Error: " . $err);[m
[32m+[m[32m        return ['status' => 'error', 'message' => "cURL Error: " . $err];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Log the raw API response for debugging purposes[m
[32m+[m[32m    \Log::info('API Response: ', ['response' => $response]);[m
[32m+[m
[32m+[m[32m    // Parse the API response[m
[32m+[m[32m    $decodedResponse = json_decode($response, true);[m
[32m+[m
[32m+[m[32m    // Check if the JSON response is valid[m
[32m+[m[32m    if (json_last_error() !== JSON_ERROR_NONE) {[m
[32m+[m[32m        return ['status' => 'error', 'message' => 'Invalid JSON response.'];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Check if the 'status' key exists in the response[m
[32m+[m[32m    if (isset($decodedResponse['status'])) {[m
[32m+[m[32m        // Handle a successful response[m
[32m+[m[32m        if ($decodedResponse['status'] === 'success') {[m
[32m+[m[32m            return ['status' => 'success', 'message' => $decodedResponse['data']]; // Assuming 'data' contains the response details[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return ['status' => 'error', 'message' => $decodedResponse['message']]; // Return the error message from the API[m
[32m+[m[32m        }[m
[32m+[m[32m    } else {[m
[32m+[m[32m        return ['status' => 'error', 'message' => 'Unexpected response structure.'];[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m    // handle the request from twillio side[m[41m [m
[32m+[m[32m    public function handleIncomingMessage(Request $request)[m
[32m+[m[32m    {[m
[32m+[m[32m        $from = $request->input('From'); // Sender's number[m
[32m+[m[32m        $userMessage = trim($request->input('Body')); // Message content[m
[32m+[m
[32m+[m[32m        \Log::info("Received message from {$from}: {$userMessage}");[m
[32m+[m
[32m+[m[32m        if (!isset($this->sessionData[$from])) {[m
[32m+[m[32m            $this->sessionData[$from] = ['menu_state' => 'main']; // Initialize session data[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $responseMessage = $this->processUserMessage($userMessage, $from);[m
[32m+[m
[32m+[m[32m        \Log::info("Response message for {$from}: {$responseMessage}");[m
[32m+[m
[32m+[m[32m        $this->sendMessage($from, $responseMessage);[m
[32m+[m[32m        return response()->xml(['Message' => $responseMessage]);[m
[32m+[m[32m    }[m
[32m+[m
